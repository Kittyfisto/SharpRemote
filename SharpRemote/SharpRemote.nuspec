<?xml version="1.0"?>
<package >
  <metadata>
    <id>SharpRemote</id>
    <!-- The version will be overwritten via a commandline parameter in pack.cmd.
         However if we don't include a placeholder here, then nuget will throw a hissy fit
         and not produce anything... -->
    <version>$version$</version>
    <authors>Simon Mießler</authors>
    <owners>Simon Mießler</owners>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/Kittyfisto/SharpRemote/</projectUrl>
    <!--<iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>-->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>.NET Remoting library</description>
    <releaseNotes>Alpha release</releaseNotes>
    <copyright>Copyright 2017</copyright>
    <tags>.NET Remoting RPC</tags>
    <dependencies>
      <!--<dependency id="SampleDependency" version="1.0" />-->
    </dependencies>
  </metadata>
  <files>
    <file src="..\bin\Win\SharpRemote.XML" target="lib\net45\" />
    <file src="..\bin\Win\SharpRemote.pdb" target="lib\net45\" />
    <file src="..\bin\Win\SharpRemote.Host.exe" target="lib\net45\" />
    <file src="..\bin\Win\SharpRemote.Host.pdb" target="lib\net45\" />
  </files>
</package>
